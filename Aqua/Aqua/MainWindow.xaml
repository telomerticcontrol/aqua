					<Rectangle.Fill>
						<LinearGradientBrush
					StartPoint="0.5,0"
					EndPoint="0.5,1">
					<LinearGradientBrush.GradientStops>
						<GradientStopCollection>
							<GradientStop
								Color="#FFffffff"
								Offset="0" />
							<GradientStop
								Color="#FFdbdbdb"
								Offset="1" />
						</GradientStopCollection>
					</LinearGradientBrush.GradientStops>
				</LinearGradientBrush>
					</Rectangle.Fill>
				</Rectangle>
				<view:LiveLoginView
				x:Name="LiveLoginView1"
				Grid.RowSpan="2"
				HorizontalAlignment="Center"
				VerticalAlignment="Center"
				DataContext="{Binding}" RenderTransformOrigin="0.5,0.5" IsHitTestVisible="False" Visibility="Collapsed" Opacity="0">
				<view:LiveLoginView.RenderTransform>
					<TransformGroup>
						<ScaleTransform/>
						<SkewTransform/>
						<RotateTransform/>
						<TranslateTransform/>
					</TransformGroup>
				</view:LiveLoginView.RenderTransform>
			</view:LiveLoginView>
		</Grid>
	</Grid>
</Window>