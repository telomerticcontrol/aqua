<UserControl x:Class="Infragistics.Guidance.Aqua.Client.View.PatientRecordView"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:igWindows="http://infragistics.com/Windows" 
    xmlns:igControls="clr-namespace:Infragistics.Silverlight.Controls;assembly=Infragistics.Silverlight.DataVisualization.Controls.v9.2"
    xmlns:igtl="clr-namespace:Infragistics.Silverlight.Timeline;assembly=Infragistics.Silverlight.DataVisualization.Timeline.v9.2"
    xmlns:local="clr-namespace:Infragistics.Guidance.Aqua.Client.Converters"
             HorizontalAlignment="Stretch" VerticalAlignment="Stretch">
	<UserControl.Resources>
		<local:AvailableSizeToItemsPerPageConverter x:Key="AvailableSizeToItemsPerPageConverter"/>
		<Style x:Key="AdmittanceDateItemContainerStyle" TargetType="{x:Type ListBoxItem}">
			<Setter Property="Background" Value="Transparent"/>
			<Setter Property="HorizontalContentAlignment" Value="{Binding HorizontalContentAlignment, RelativeSource={RelativeSource AncestorType={x:Type ItemsControl}}}"/>
			<Setter Property="VerticalContentAlignment" Value="{Binding VerticalContentAlignment, RelativeSource={RelativeSource AncestorType={x:Type ItemsControl}}}"/>
			<Setter Property="Padding" Value="2,0,0,0"/>
			<Setter Property="Template">
				<Setter.Value>
					<ControlTemplate TargetType="{x:Type ListBoxItem}">
						<Border x:Name="Bd" SnapsToDevicePixels="true" BorderBrush="{TemplateBinding BorderBrush}" BorderThickness="{TemplateBinding BorderThickness}" Padding="{TemplateBinding Padding}">
							<ContentPresenter HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}" VerticalAlignment="{TemplateBinding VerticalContentAlignment}" SnapsToDevicePixels="{TemplateBinding SnapsToDevicePixels}"/>
						</Border>
						<ControlTemplate.Triggers>
							<Trigger Property="IsMouseOver" Value="True">
								<Setter Property="Background" TargetName="Bd" Value="#FFD4D4D4"/>
							</Trigger>
							<MultiTrigger>
								<MultiTrigger.Conditions>
									<Condition Property="IsSelected" Value="true"/>
									<Condition Property="Selector.IsSelectionActive" Value="false"/>
								</MultiTrigger.Conditions>
								<Setter Property="Background" TargetName="Bd" Value="{DynamicResource {x:Static SystemColors.ControlBrushKey}}"/>
								<Setter Property="Foreground" Value="{DynamicResource {x:Static SystemColors.ControlTextBrushKey}}"/>
							</MultiTrigger>
							<Trigger Property="IsEnabled" Value="false">
								<Setter Property="Foreground" Value="{DynamicResource {x:Static SystemColors.GrayTextBrushKey}}"/>
							</Trigger>
							<Trigger Property="IsSelected" Value="true">
								<Setter Property="Foreground" Value="{DynamicResource {x:Static SystemColors.HighlightTextBrushKey}}"/>
								<Setter Property="Background" TargetName="Bd" Value="#FFFF6600"/>
							</Trigger>

						</ControlTemplate.Triggers>
					</ControlTemplate>
				</Setter.Value>
			</Setter>
		</Style>
	</UserControl.Resources>
	<Grid>
		<Grid.Resources>
			<LinearGradientBrush x:Key="ScrollFill" EndPoint="0,1" StartPoint="0,0">
				<GradientStop Color="#FF999999" Offset="0"/>
				<GradientStop Color="#FF022041" Offset="1"/>
			</LinearGradientBrush>
			<SolidColorBrush x:Key="ScrollStroke" Color="#FF333333"/>
			<LinearGradientBrush x:Key="EventTitleFill" StartPoint="0,0" EndPoint="0,1">
				<LinearGradientBrush.GradientStops>
					<GradientStopCollection>
						<GradientStop Offset="0" Color="#FFffffff"/>
						<GradientStop Offset="0.4" Color="#FFf4f4f4"/>
						<GradientStop Offset="0.95" Color="#FFE3E3E3"/>
						<GradientStop Offset="1" Color="#FFe2e2e2"/>
					</GradientStopCollection>
				</LinearGradientBrush.GradientStops>
			</LinearGradientBrush>
			<SolidColorBrush x:Key="EventTitleStroke" Color="#FFD4D4D4"/>
			<LinearGradientBrush x:Key="AxisPaneFill" StartPoint="0,0" EndPoint="0,1">
				<LinearGradientBrush.GradientStops>
					<GradientStopCollection>
						<GradientStop Offset="0" Color="#FFffffff"/>
						<GradientStop Offset="0.300" Color="#FFffffff"/>
						<GradientStop Offset="1" Color="#FF9f9f9f"/>
					</GradientStopCollection>
				</LinearGradientBrush.GradientStops>
			</LinearGradientBrush>
			<SolidColorBrush x:Key="AxisPaneStroke" Color="#FFDDDDDD"/>
			<LinearGradientBrush x:Key="EventPointFill" StartPoint="0,0" EndPoint="0,1">
				<LinearGradientBrush.GradientStops>
					<GradientStopCollection>
						<GradientStop Offset="0" Color="#FFffffff"/>
						<GradientStop Offset="0.400" Color="#FFf4f4f4"/>
						<GradientStop Offset="0.600" Color="#FFe2e2e2"/>
						<GradientStop Offset="0.944" Color="#FFEEEEEE"/>
						<GradientStop Offset="1" Color="#FFf0f0f0"/>
					</GradientStopCollection>
				</LinearGradientBrush.GradientStops>
			</LinearGradientBrush>
			<SolidColorBrush x:Key="EventPointStroke" Color="#FFDDDDDD"/>
			<SolidColorBrush x:Key="MarkFill" Color="#FF989898"/>

			<local:EventTitleToPatientRecordItem x:Key="EventTitleToPatientRecordItemConverter" />
			<DataTemplate x:Key="AdmittanceListBoxItemTemplate">
				<Border x:Name="Bd" SnapsToDevicePixels="true" Width="Auto">
					<Grid>
						<TextBlock
						    FontFamily="Calibri"
						    FontSize="14"
						    TextWrapping="Wrap"
						    Text="{Binding AdmittanceDate, StringFormat={}{0:MM/dd/yyyy}}" />
					</Grid>
				</Border>
			</DataTemplate>
			<DataTemplate x:Key="ListBoxItemTemplate">
				<Border x:Name="Bd" SnapsToDevicePixels="true" Width="Auto">

					<TextBlock
						    FontFamily="Calibri"
						    FontSize="12"
						    Foreground="#FF767676"
						    TextWrapping="Wrap"
						    Text="{Binding Path=Note}"
						    Grid.Row="1" />
				</Border>
			</DataTemplate>
			<DataTemplate x:Key="ImageCarouselItemTemplate">
				<Border>
					<Grid>
						<Grid.RowDefinitions>
							<RowDefinition Height="20" />
							<RowDefinition />
						</Grid.RowDefinitions>
						<Grid.ColumnDefinitions>
							<ColumnDefinition />
						</Grid.ColumnDefinitions>
						<StackPanel Orientation="Horizontal" HorizontalAlignment="Center" >
							<TextBlock x:Name="txtTestType" HorizontalAlignment="Left" Margin="0,0,5,0" FontSize="12" FontWeight="Bold" Foreground="#FFFF9900" Text="{Binding Type}" TextWrapping="Wrap" VerticalAlignment="Center"/>
							<TextBlock x:Name="txtTestTimestamp" HorizontalAlignment="Left" Text="{Binding Timestamp}" TextWrapping="Wrap" Foreground="#FF666666" VerticalAlignment="Center"/>
						</StackPanel>
						<Image Grid.Row="1" Grid.ColumnSpan="1" Source="{Binding Image}" Height="200" Width="200" HorizontalAlignment="Center" VerticalAlignment="Center" />
					</Grid>
				</Border>
			</DataTemplate>
			<Style x:Key="TickmarkStyle" TargetType="igtl:Mark">
				<Setter Property="Width" Value="2"/>
				<Setter Property="Height" Value="10" />
				<Setter Property="Fill" Value="{StaticResource MarkFill}"/>
				<Setter Property="Stroke" Value="{StaticResource MarkFill}"/>
				<Setter Property="IsTabStop" Value="False"/>
				<Setter Property="Template">
					<Setter.Value>
						<ControlTemplate TargetType="igtl:Mark">
							<Rectangle Fill="{TemplateBinding Fill}" Stroke="{TemplateBinding Stroke}"/>
						</ControlTemplate>
					</Setter.Value>
				</Setter>
			</Style>
			<Style x:Key="ZoombarStyle" TargetType="igControls:XamWebZoombar">
				<Setter Property="Foreground" Value="#FF333333"/>
				<Setter Property="Template">
					<Setter.Value>
						<ControlTemplate TargetType="igControls:XamWebZoombar">
							<Grid>
								<Canvas x:Name="HorizontalRootCanvasElement">
									<Grid x:Name="HorizontalThumbElement" Style="{TemplateBinding HorizontalThumbStyle}">
										<Border Background="{StaticResource ScrollFill}" BorderBrush="{StaticResource ScrollStroke}" BorderThickness="0,1,0,1" Opacity="0.35" Margin="4,0"/>
									</Grid>
									<Grid x:Name="HorizontalScaleLeftElement" Cursor="SizeWE" Style="{TemplateBinding HorizontalScaleLeftStyle}" Width="8" Background="Transparent">
										<Border Background="{StaticResource ScrollFill}" BorderBrush="{StaticResource ScrollStroke}" BorderThickness="1,1,0,1" Opacity="0.35"/>
										<Border BorderThickness="1,0,0,0" BorderBrush="#FF666666" Background="#FFFFFFFF" Width="2" Height="10" HorizontalAlignment="Center" VerticalAlignment="Center"/>
									</Grid>
									<Grid x:Name="HorizontalScaleRightElement" Cursor="SizeWE" Style="{TemplateBinding HorizontalScaleRightStyle}" Width="8" Background="Transparent">
										<Border Background="{StaticResource ScrollFill}" BorderBrush="{StaticResource ScrollStroke}" BorderThickness="0,1,1,1" Opacity="0.35"/>
										<Border BorderThickness="0,0,1,0" BorderBrush="#FF666666" Background="#FFFFFFFF" Width="2" Height="10" HorizontalAlignment="Center" VerticalAlignment="Center"/>
									</Grid>
									<Grid x:Name="HorizontalScrollbarElement">
										<Rectangle Opacity="0.2"  Stroke="{StaticResource ScrollStroke}" StrokeThickness="1" RadiusX="1" RadiusY="1"/>
										<Grid>
											<Grid.ColumnDefinitions>
												<ColumnDefinition Width="30"/>
												<ColumnDefinition/>
												<ColumnDefinition Width="30"/>
											</Grid.ColumnDefinitions>
											<Grid.RowDefinitions>
												<RowDefinition Height="0.5*"/>
												<RowDefinition Height="0.5*"/>
											</Grid.RowDefinitions>
											<Border x:Name="HorizontalTrackElement" />
											<Grid Cursor="Hand" Grid.RowSpan="2">
												<Border x:Name="HorizontalScrollLeftElement" Background="Transparent"/>
												<Path
												HorizontalAlignment="Center"
												Margin="0,0,0,0"
												VerticalAlignment="Center"
												Data="M 17,0 C17,0 17,31 17,31 17,31 0,15 0,15 0,15 17,0 17,0 z"
												Stretch="Fill"
												Stroke="{StaticResource ScrollStroke}"
												StrokeThickness="1" Height="25" Width="12" Opacity="0.3" SnapsToDevicePixels="True" Fill="{StaticResource ScrollFill}" IsHitTestVisible="False"/>
											</Grid>
											<Grid Cursor="Hand" Grid.Column="2" Grid.RowSpan="2">
												<Border x:Name="HorizontalScrollRightElement" Background="Transparent"/>
												<Path
												HorizontalAlignment="Center"
												Margin="0,0,0,0"
												VerticalAlignment="Center"
												Data="M 0,0 C0,0 0,31 0,31 0,31 17,15 17,15 17,15 0,0 0,0 z"
												Stretch="Fill"
												Stroke="{StaticResource ScrollStroke}"
												StrokeThickness="1" Height="25" Width="12" Opacity="0.3" SnapsToDevicePixels="True" Fill="{StaticResource ScrollFill}" IsHitTestVisible="False"/>
											</Grid>
										</Grid>
										<Grid x:Name="HorizontalPreviewElement" Margin="36,0,36,5">
											<ContentPresenter Content="{TemplateBinding HorizontalPreviewContent}"/>
										</Grid>
									</Grid>
								</Canvas>
								<Canvas x:Name="VerticalRootCanvasElement" Visibility="Collapsed" Background="{TemplateBinding Background}">
									<Grid x:Name="VerticalScrollbarElement">
										<Grid>
											<Grid.ColumnDefinitions>
												<ColumnDefinition Width="0.5*"/>
												<ColumnDefinition Width="0.5*"/>
											</Grid.ColumnDefinitions>
											<Grid.RowDefinitions>
												<RowDefinition Height="30"/>
												<RowDefinition/>
												<RowDefinition Height="30"/>
											</Grid.RowDefinitions>
											<Border x:Name="VerticalTrackElement"/>
											<Grid Cursor="Hand" Grid.ColumnSpan="2">
												<Border x:Name="VerticalScrollTopElement" Background="Transparent"/>
											</Grid>
											<Grid Cursor="Hand" Grid.ColumnSpan="2" Grid.Row="2">
												<Border x:Name="VerticalScrollBottomElement" Background="Transparent"/>
											</Grid>
										</Grid>
										<Grid x:Name="VerticalPreviewElement" Margin="20,33,5,33">
											<ContentPresenter Content="{TemplateBinding VerticalPreviewContent}"/>
										</Grid>
									</Grid>
									<Grid x:Name="VerticalThumbElement" Style="{TemplateBinding VerticalThumbStyle}"/>
									<Grid x:Name="VerticalScaleTopElement" Cursor="SizeNS" Height="8" Style="{TemplateBinding VerticalScaleTopStyle}"/>
									<Grid x:Name="VerticalScaleBottomElement" Cursor="SizeNS" Height="8" Style="{TemplateBinding VerticalScaleBottomStyle}"/>
								</Canvas>
							</Grid>
						</ControlTemplate>
					</Setter.Value>
				</Setter>
			</Style>
			<Style x:Key="LabelStyle" TargetType="igtl:AxisLabel">
				<Setter Property="IsTabStop" Value="False"/>
				<Setter Property="Foreground" Value="#FF666666"/>
				<Setter Property="Template">
					<Setter.Value>
						<ControlTemplate TargetType="igtl:AxisLabel">
							<TextBlock Foreground="{TemplateBinding Foreground}" x:Name="TextBlockElement" Text="{TemplateBinding FormattedText}"/>
						</ControlTemplate>
					</Setter.Value>
				</Setter>
			</Style>
			<Style x:Key="EventPointStyle" TargetType="igtl:EventPoint">
				<Setter Property="Width" Value="12" />
				<Setter Property="Stroke" Value="{StaticResource EventPointStroke}"/>
				<Setter Property="Fill" Value="{StaticResource EventPointFill}"/>
				<Setter Property="HollowFill" Value="{StaticResource EventPointFill}"/>
				<Setter Property="IsTabStop" Value="False"/>
				<Setter Property="Cursor" Value="Hand"/>
				<Setter Property="Template">
					<Setter.Value>
						<ControlTemplate TargetType="igtl:EventPoint">
							<Grid Width="{TemplateBinding Width}" Height="{TemplateBinding Width}">
								<Ellipse Fill="{TemplateBinding Fill}" Stroke="{TemplateBinding Stroke}" StrokeThickness="1"/>
								<Ellipse Stroke="#FFFFFFFF" Margin="1,1,1,1"/>
							</Grid>
						</ControlTemplate>
					</Setter.Value>
				</Setter>
			</Style>
			<Style x:Key="AxisPaneStyle" TargetType="{x:Type igtl:Axis}">
				<Setter Property="PaneStyle">
					<Setter.Value>
						<Style TargetType="igtl:Mark">
							<Setter Property="Height" Value="6"/>
							<Setter Property="Template">
								<Setter.Value>
									<ControlTemplate TargetType="igtl:Mark">
										<Rectangle Fill="{StaticResource AxisPaneFill}" 
											    Stroke="{StaticResource AxisPaneStroke}" 
											    RadiusX="3" 
											    RadiusY="3"/>
									</ControlTemplate>
								</Setter.Value>
							</Setter>
						</Style>
					</Setter.Value>
				</Setter>
			</Style>
			<Style x:Key="TitleStyle" TargetType="{x:Type igtl:EventTitle}">
				<Setter Property="Background" Value="{StaticResource EventTitleFill}"/>
				<Setter Property="BorderBrush" Value="{StaticResource EventTitleStroke}"/>
				<Setter Property="Foreground" Value="#FF666666"/>
				<Setter Property="Width" Value="105"/>
				<Setter Property="Template">
					<Setter.Value>
						<ControlTemplate TargetType="{x:Type igtl:EventTitle}">
							<Grid>
								<Border BorderBrush="{TemplateBinding BorderBrush}" BorderThickness="1,1,2,1" CornerRadius="5" Padding="5" SnapsToDevicePixels="True" Background="{TemplateBinding Background}">
									<Grid>
										<Grid.RowDefinitions>
											<RowDefinition Height="Auto" ></RowDefinition>
											<RowDefinition Height="Auto"></RowDefinition>
											<RowDefinition Height="*"></RowDefinition>
										</Grid.RowDefinitions>
										<TextBlock Text="{TemplateBinding Title, Converter={StaticResource EventTitleToPatientRecordItemConverter}, ConverterParameter=Date}" Grid.Row="0" TextWrapping="Wrap" FontFamily="Calibri" FontSize="14" FontWeight="Bold" Foreground="#FF1FA4FC"/>
										<TextBlock Text="{TemplateBinding Title, Converter={StaticResource EventTitleToPatientRecordItemConverter}, ConverterParameter=Diagnosis}" Grid.Row="1" />
										<ScrollViewer Grid.Row="2"  BorderThickness="0" Background="Transparent" VerticalScrollBarVisibility="Auto">
											<ListBox ItemsSource="{TemplateBinding Title, Converter={StaticResource EventTitleToPatientRecordItemConverter}, ConverterParameter=Notes}" Grid.Row="2" ItemTemplate="{StaticResource ListBoxItemTemplate}" BorderThickness="0" Background="Transparent" ScrollViewer.HorizontalScrollBarVisibility="Disabled" ScrollViewer.VerticalScrollBarVisibility="Auto"/>
										</ScrollViewer>
									</Grid>
								</Border>
								<Rectangle RadiusX="4" RadiusY="4" Stroke="#FFFFFFFF" StrokeThickness="1" Margin="1,1,2,1" SnapsToDevicePixels="True" />
							</Grid>
						</ControlTemplate>
					</Setter.Value>
				</Setter>
			</Style>

			<Style x:Key="SceneStyle" TargetType="igtl:Scene">
				<Setter Property="IsTabStop" Value="False"/>
				<Setter Property="Template">
					<Setter.Value>
						<ControlTemplate TargetType="igtl:Scene">
							<Grid>
								<igtl:GridArea x:Name="GridAreaElement" />
								<Grid Style="{TemplateBinding SceneGridStyle}">
									<Grid.RowDefinitions>
										<RowDefinition Height="*" />
										<RowDefinition Height="Auto" />
										<RowDefinition Height="0" />
									</Grid.RowDefinitions>
									<igtl:SeriesPane x:Name="SeriesPaneTopElement" />
									<Grid x:Name="AxisPaneElement" Height="50" Grid.Row="1" Margin="0,0,0,5">
									</Grid>
									<igtl:SeriesPane x:Name="SeriesPaneBottomElement" 
                                              Grid.Row="2" />
								</Grid>
							</Grid>
						</ControlTemplate>
					</Setter.Value>
				</Setter>
			</Style>

			<Style x:Key="TimelineStyle" TargetType="igtl:XamWebTimeline">
				<Setter Property="Template">
					<Setter.Value>
						<ControlTemplate TargetType="igtl:XamWebTimeline">
							<Grid>
								<Grid>
									<Grid.RowDefinitions>
										<RowDefinition Height="Auto" />
										<RowDefinition Height="*" />
									</Grid.RowDefinitions>
									<igtl:Title Content="{TemplateBinding Title}" 
                                         Style="{TemplateBinding TitleStyle}" />
									<Grid Grid.Row="1">
										<Grid.ColumnDefinitions>
											<ColumnDefinition Width="*" />
											<ColumnDefinition Width="Auto" />
										</Grid.ColumnDefinitions>
										<Grid.RowDefinitions>
											<RowDefinition Height="*" />
											<RowDefinition Height="Auto"/>
										</Grid.RowDefinitions>
										<igtl:Legend x:Name="LegendElement" 
                                          Title="{TemplateBinding LegendTitle}"
                                          Style="{TemplateBinding LegendStyle}"
                                          Grid.Column="1"
                                          Margin="10,0,0,0"/>
										<Grid>
											<Border 
											Background="{TemplateBinding Background}"
											BorderBrush="{TemplateBinding BorderBrush}"
											BorderThickness="{TemplateBinding BorderThickness}">
												<Grid>
													<igtl:Scene x:Name="SceneElement" 
													 Style="{TemplateBinding SceneStyle}" 
													 Grid.Column="0" Margin="5" />
													<Canvas x:Name="DetailsPaneElement" />
												</Grid>
											</Border>
										</Grid>
										<Grid x:Name="ZoombarPanelElement" Height="45" Grid.Row="1" />
									</Grid>
								</Grid>
								<igtl:MessageControl x:Name="MessageControlElement"
                                            Style="{TemplateBinding MessageControlStyle}" />
							</Grid>
						</ControlTemplate>
					</Setter.Value>
				</Setter>
			</Style>
